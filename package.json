{
    "name": "dims-api",
    "title": "dims",
    "version": "0.0.1",
    "description": "dev incubator management system back-end",
    "author": "maximsan",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "clean": "rimraf node_modules",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:dev": "cross-env NODE_ENV=development nest start",
        "start:dev:watch": "yarn start:dev --watch",
        "start:debug": "yarn start:dev --debug --watch",
        "docker:up": "docker-compose --env-file=docker-dev.env -f docker-compose.yml up -d",
        "docker:down": "docker-compos --env-file=docker-dev.env -f docker-compose.yml down --volumes",
        "start:dev:docker": "cross-env NODE_ENV=development concurrently \"nest start\" \"yarn docker:up\",",
        "start:dev:docker:watch": "cross-env NODE_ENV=development concurrently \"nest start --watch\" \"yarn docker:up\"",
        "start:prod": "yarn db:setup:prod && node dist/src/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config e2e/jest-e2e.json",
        "test:e2e:watch": "jest --config e2e/jest-e2e.json --watch",
        "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:create": "typeorm migration:create -d src/database/migrations -p -n",
        "migration:generate:dev": "cross-env NODE_ENV=development yarn build && cross-env NODE_ENV=development yarn typeorm migration:generate -p -n",
        "migration:run:dev": "cross-env NODE_ENV=development yarn build && cross-env NODE_ENV=development typeorm migration:run",
        "migration:revert": "typeorm migration:revert",
        "migration:run:prod": "cross-env NODE_ENV=production typeorm migration:run",
        "seed:config": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "cross-env NODE_ENV=development yarn build && cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "seed:run:prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:drop:prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:sync": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js schema:sync",
        "schema:log": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js schema:log",
        "db:setup": "yarn schema:drop && yarn run migration:run:dev && yarn run seed:run",
        "db:setup:prod": "yarn schema:drop:prod && yarn migration:run:prod && yarn seed:run:prod",
        "prepare": "husky install"
    },
    "dependencies": {
        "@automapper/classes": "^8.3.6",
        "@automapper/core": "^8.3.6",
        "@automapper/nestjs": "^8.3.6",
        "@nestjs/common": "8.4.4",
        "@nestjs/config": "2.0.0",
        "@nestjs/core": "8.4.4",
        "@nestjs/jwt": "8.0.0",
        "@nestjs/mapped-types": "1.0.1",
        "@nestjs/passport": "8.2.1",
        "@nestjs/platform-express": "8.4.4",
        "@nestjs/swagger": "5.2.1",
        "@nestjs/typeorm": "8.0.3",
        "@types/bcrypt": "5.0.0",
        "@types/passport-jwt": "3.0.6",
        "bcrypt": "5.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "concurrently": "7.3.0",
        "cross-env": "7.0.3",
        "dotenv": "10.0.0",
        "joi": "17.8.4",
        "morgan": "1.10.0",
        "passport": "0.5.2",
        "passport-jwt": "4.0.0",
        "passport-local": "1.0.0",
        "pg": "8.6.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.5",
        "swagger-ui-express": "4.4.0",
        "ts-node": "10.9.1",
        "typeorm": "0.2.34",
        "typeorm-seeding": "1.6.1",
        "typescript": "4.6.4"
    },
    "devDependencies": {
        "@nestjs/cli": "8.2.5",
        "@nestjs/schematics": "8.0.11",
        "@nestjs/testing": "8.4.4",
        "@types/express": "4.17.13",
        "@types/faker": "5.5.7",
        "@types/jest": "26.0.24",
        "@types/morgan": "1.9.3",
        "@types/node": "18.6.1",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "4.28.3",
        "eslint": "7.31.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.4",
        "jest": "26.6.3",
        "prettier": "2.6.2",
        "supertest": "6.2.3",
        "ts-jest": "26.5.6",
        "ts-loader": "9.3.0",
        "tsconfig-paths": "3.9.0"
    },
    "engines": {
        "node": "16.x",
        "yarn": "1.x",
        "npm": "8.x"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*.(spec|test).ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "volta": {
        "node": "16.15.0"
    }
}
